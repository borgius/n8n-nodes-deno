{
  "name": "n8n-nodes-deno-code",
  "version": "0.2.0",
  "description": "n8n node for Deno - JavaScript, TypeScript, and WebAssembly runtime with secure defaults and a great developer experience https://deno.com/",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/chudnyi/n8n-nodes-deno-code",
  "author": {
    "name": "Dmitriy Chudnyi",
    "email": "dmitriy@chudnyi.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chudnyi/n8n-nodes-deno-code.git"
  },
  "engines": {
    "node": "18"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run clean && npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json",
    "dev:install": "npm link && mkdir -p $HOME/.n8n/custom/nodes && cd $HOME/.n8n/custom/nodes && npm link n8n-nodes-deno-code --legacy-peer-deps",
    "dev:start": "pkgx +node@18 -- n8n start -o",
    "dev:publish:dry-run": "npm run dev:publish -- --dry-run",
    "dev:publish": "npm publish --tag dev",
    "dev:unpublish": "npm unpublish n8n-nodes-deno-code@$(node --print 'require(`./package.json`).version')"
  },
  "files": [
    "dist/nodes/DenoCode/deno.svg",
    "dist/nodes/DenoCode/DenoCode.node.js"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/DenoCode/DenoCode.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "rimraf": "5.0.5",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@chudnyi/deno-vm": "0.12.0",
    "deno-bin": "1.42.4"
  }
}
